{
	"singleQuote": true,
	"trailingComma": "all",
	"useTabs": true,
	"semi": true,
	"bracketSpacing": true,
	"printWidth": 100,
	"endOfLine": "auto",
	"importOrder": [
		"^react$",
		"<THIRD_PARTY_MODULES>",
		"^@/assets/(.*)$",
		"^@/layout/(.*)$",
		"^@/pages/(.*)$",
		"^@/widgets/(.*)$",
		"^@/features/(.*)$",
		"^@/aentities/(.*)$",
		"^@/routers/(.*)$",
		"^@/styles/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(^.module.*)",
		"(.scss)"
	],
	"importOrderGroupNamespaceSpecifiers": true,
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
